{
  "name": "@m2c2kit/assessment-symbol-search",
  "version": "0.8.17",
  "m2c2kit": {
    "assessmentId": "symbol-search"
  },
  "description": "A speeded continuous performance test of conjunctive feature search in which respondents identify matching symbol pairs as quickly and as accurately as they can",
  "scripts": {
    "build": "npm run clean && tsc && rollup -c && npm run extract-schemas",
    "extract-schemas": "node ../schema-util/build/index.js list --schema=all --files=src/index.ts --format=json-schema --title=\"Symbol Search version __VERSION__\" --game-class=../core/src/Game.ts > schemas.json",
    "clean": "rimraf build dist .rollup.cache tsconfig.tsbuildinfo"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "!*.d.ts.map",
    "assets",
    "schemas.json",
    "metadata.json"
  ],
  "license": "MIT",
  "author": {
    "name": "Scott T. Yabiku",
    "email": "syabiku@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/m2c2-project/m2c2kit.git",
    "directory": "packages/assessment-symbol-search"
  },
  "homepage": "https://m2c2-project.github.io/m2c2kit",
  "dependencies": {
    "@m2c2kit/addons": "0.3.16",
    "@m2c2kit/core": "0.3.19"
  },
  "devDependencies": {
    "@m2c2kit/build-helpers": "0.3.16",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-replace": "5.0.7",
    "rimraf": "6.0.1",
    "rollup": "4.18.1",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-esbuild": "6.1.1",
    "typescript": "5.5.3"
  },
  "engines": {
    "node": ">=18"
  }
}
