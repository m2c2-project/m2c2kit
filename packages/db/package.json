{
  "name": "@m2c2kit/db",
  "version": "0.3.1",
  "description": "A local database for storing m2c2kit data on the device. It is built using Dexie.js, and the underlying client-side storage is IndexedDB.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/data.js",
    "data.html"
  ],
  "scripts": {
    "build": "npm run clean && rollup -c",
    "clean": "rimraf build dist .rollup.cache tsconfig.tsbuildinfo",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "24.0.1",
    "@rollup/plugin-node-resolve": "15.0.2",
    "@rollup/plugin-typescript": "11.1.0",
    "rollup": "3.20.2",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-dts": "5.3.0",
    "typescript": "5.0.3"
  },
  "dependencies": {
    "@m2c2kit/core": "0.3.4",
    "dexie": "3.2.3"
  }
}
