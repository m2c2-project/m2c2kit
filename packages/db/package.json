{
  "name": "@m2c2kit/db",
  "version": "0.3.7",
  "description": "A local database for storing m2c2kit data on the device. It is built using Dexie.js, and the underlying client-side storage is IndexedDB.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/data.js",
    "data.html"
  ],
  "scripts": {
    "build": "npm run clean && tsc --build && rollup -c",
    "clean": "rimraf build build-nobundler dist .rollup.cache tsconfig.tsbuildinfo",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "15.1.0",
    "rimraf": "5.0.1",
    "rollup": "3.25.1",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-dts": "5.3.0",
    "rollup-plugin-esbuild": "5.0.0",
    "typescript": "5.1.3"
  },
  "dependencies": {
    "@m2c2kit/core": "^0.3.6",
    "dexie": "3.2.4"
  }
}
