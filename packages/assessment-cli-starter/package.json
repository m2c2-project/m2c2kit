{
  "name": "@m2c2kit/assessment-cli-starter",
  "version": "0.8.16",
  "m2c2kit": {
    "assessmentId": "cli-starter"
  },
  "description": "A simple Stroop assessment created when the CLI creates a new app",
  "scripts": {
    "build": "npm run clean && tsc && rollup -c",
    "clean": "rimraf build dist .rollup.cache tsconfig.tsbuildinfo"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "!*.d.ts.map",
    "assets",
    "metadata.json"
  ],
  "license": "MIT",
  "author": {
    "name": "Scott T. Yabiku",
    "email": "syabiku@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/m2c2-project/m2c2kit.git",
    "directory": "packages/assessment-cli-starter"
  },
  "homepage": "https://m2c2-project.github.io/m2c2kit",
  "dependencies": {
    "@m2c2kit/addons": "0.3.16",
    "@m2c2kit/core": "0.3.19"
  },
  "devDependencies": {
    "@m2c2kit/build-helpers": "0.3.16",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-replace": "5.0.7",
    "rimraf": "6.0.1",
    "rollup": "4.18.1",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-esbuild": "6.1.1",
    "typescript": "5.5.3"
  },
  "engines": {
    "node": ">=18"
  }
}
