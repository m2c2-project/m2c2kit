{
  "name": "@m2c2kit/assessment-cli-starter",
  "version": "0.8.9",
  "description": "A simple Stroop assessment created when the CLI creates a new app",
  "scripts": {
    "build": "npm run clean && tsc && rollup -c",
    "clean": "rimraf build build-nobundler dist .rollup.cache tsconfig.tsbuildinfo"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "assets/**"
  ],
  "license": "MIT",
  "author": {
    "name": "Scott T. Yabiku",
    "email": "syabiku@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/m2c2-project/m2c2kit.git",
    "directory": "packages/assessment-cli-starter"
  },
  "homepage": "https://m2c2-project.github.io/m2c2kit",
  "dependencies": {
    "@m2c2kit/addons": "^0.3.3",
    "@m2c2kit/core": "^0.3.6"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "15.1.0",
    "@rollup/plugin-replace": "5.0.2",
    "rimraf": "5.0.1",
    "rollup": "3.25.3",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-esbuild": "5.0.0",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": ">=18"
  }
