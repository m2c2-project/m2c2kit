{
  "name": "@m2c2kit/cli",
  "version": "0.1.4",
  "description": "m2c2kit command line interface",
  "module": "dist/cli.js",
  "files": [
    "dist"
  ],
  "type": "module",
  "bin": {
    "m2": "./dist/cli.js"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run compile && npm run copy-files && npm run write-dotenv",
    "compile": "tsc",
    "clean": "rimraf dist/ && rimraf build/",
    "copy-files": "copyfiles -f build/src/cli.js dist && copyfiles templates/* fonts/**/* dist/",
    "write-dotenv": "node write-dotenv.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "0.24.0",
    "chalk": "5.0.0",
    "conf": "10.1.1",
    "form-data": "4.0.0",
    "handlebars": "4.7.7",
    "ora": "6.0.1",
    "prompts": "2.4.2",
    "yargs": "17.3.0"
  },
  "devDependencies": {
    "@types/node": "16.11.12",
    "@types/prompts": "2.0.14",
    "@types/yargs": "17.0.7",
    "copyfiles": "2.4.1",
    "rimraf": "3.0.2",
    "tslib": "2.3.1",
    "typescript": "4.5.3"
  }
}
