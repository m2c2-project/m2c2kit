{
  "name": "@m2c2kit/assessment-color-shapes",
  "version": "0.8.1",
  "scripts": {
    "serve": "rollup -c rollup.config.mjs --watch --configServe",
    "build": "npm run clean && rollup -c",
    "clean": "rimraf build build-nobundler dist .rollup.cache tsconfig.tsbuildinfo",
    "postinstall": "node post-install.mjs"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "assets/**",
    "post-install.mjs"
  ],
  "dependencies": {
    "@m2c2kit/addons": "0.3.0",
    "@m2c2kit/core": "0.3.3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-replace": "5.0.2",
    "@rollup/plugin-typescript": "11.0.0",
    "cpy-cli": "4.2.0",
    "rimraf": "^3.0.2",
    "rollup": "3.9.1",
    "rollup-plugin-terser": "7.0.2",
    "tslib": "2.4.1",
    "typescript": "4.9.4"
  }
}
