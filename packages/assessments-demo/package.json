{
  "name": "@m2c2kit/assessments-demo",
  "version": "0.8.2",
  "scripts": {
    "serve": "rollup -c --watch --configServe",
    "build": "npm run clean && rollup -c --configProd",
    "build-webview": "npm run clean-webview && rollup -c rollup.config.webview.mjs --configProd --configNoHash",
    "build-sw": "npm run clean && rollup -c --configProd --configServiceWorker",
    "clean": "rimraf build dist .rollup.cache",
    "clean-webview": "rimraf build dist-webview .rollup.cache"
  },
  "private": true,
  "dependencies": {
    "@m2c2kit/addons": "0.3.2",
    "@m2c2kit/assessment-cli-starter": "0.8.3",
    "@m2c2kit/assessment-color-dots": "0.8.3",
    "@m2c2kit/assessment-color-shapes": "0.8.3",
    "@m2c2kit/assessment-grid-memory": "0.8.3",
    "@m2c2kit/assessment-symbol-search": "0.8.3",
    "@m2c2kit/core": "0.3.5",
    "@m2c2kit/db": "0.3.2",
    "@m2c2kit/sage-research": "0.3.2",
    "@m2c2kit/survey": "0.3.2"
  },
  "devDependencies": {
    "@m2c2kit/build-helpers": "0.3.2",
    "@rollup/plugin-commonjs": "24.1.0",
    "@rollup/plugin-node-resolve": "15.0.2",
    "@rollup/plugin-typescript": "11.1.0",
    "rollup": "3.20.4",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-livereload": "2.0.5",
    "rollup-plugin-serve": "2.0.2",
    "rollup-plugin-shim": "1.0.0",
    "rollup-plugin-sourcemaps": "0.6.3",
    "tslib": "2.5.0",
    "typescript": "5.0.4"
  }
}
