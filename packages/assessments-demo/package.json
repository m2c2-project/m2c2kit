{
  "name": "@m2c2kit/assessments-demo",
  "version": "0.8.0",
  "scripts": {
    "serve": "rollup -c --watch --configServe",
    "build": "npm run clean && rollup -c --configProd",
    "build-webview": "npm run clean-webview && rollup -c rollup.config.webview.mjs --configProd --configNoHash",
    "build-sw": "npm run clean && rollup -c --configProd --configServiceWorker",
    "clean": "rimraf build dist .rollup.cache",
    "clean-webview": "rimraf build dist-webview .rollup.cache"
  },
  "private": true,
  "dependencies": {
    "@m2c2kit/addons": "0.3.0",
    "@m2c2kit/assessment-cli-starter": "^0.8.0",
    "@m2c2kit/assessment-color-dots": "^0.8.0",
    "@m2c2kit/assessment-color-shapes": "^0.8.0",
    "@m2c2kit/assessment-grid-memory": "^0.8.0",
    "@m2c2kit/assessment-symbol-search": "^0.8.0",
    "@m2c2kit/core": "0.3.3",
    "@m2c2kit/db": "0.3.0",
    "@m2c2kit/sage-research": "^0.3.0",
    "@m2c2kit/survey": "0.3.0"
  },
  "devDependencies": {
    "@m2c2kit/build-helpers": "0.3.0",
    "@rollup/plugin-commonjs": "24.0.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-typescript": "11.0.0",
    "rollup": "3.10.0",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-livereload": "2.0.5",
    "rollup-plugin-serve": "2.0.2",
    "rollup-plugin-shim": "1.0.0",
    "rollup-plugin-sourcemaps": "0.6.3",
    "tslib": "2.4.1",
    "typescript": "4.9.4"
  }
}
