{
  "name": "m2c2kit",
  "description": "",
  "workspaces": [
    "packages/addons",
    "packages/assessment-grid-memory",
    "packages/assessment-grid-memory/demo",
    "packages/automation-controller",
    "packages/automation-driver",
    "packages/build-helpers",
    "packages/cli",
    "packages/core",
    "packages/sage-research",
    "packages/survey",
    "examples/grid-memory",
    "examples/symbol-search",
    "examples/color-dots",
    "examples/cli-starter",
    "integration-tests/*"
  ],
  "scripts": {
    "build": "npm run build -w @m2c2kit/core && npm run build -w @m2c2kit/addons && npm run build -w @m2c2kit/cli && npm run build -w @m2c2kit/sage-research  && npm run build -w @m2c2kit/build-helpers && npm run build -w grid-memory && npm run build -w symbol-search && npm run build -w color-dots && npm run build -w cli-starter",
    "docker:build": "docker build -t m2c2kit .",
    "check-m2c2kit-versions": "node check-m2c2kit-packages-versions.mjs",
    "prepare": "husky install"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "chalk": "^5.0.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "2.6.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
